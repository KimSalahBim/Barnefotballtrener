{
  "rewrites": [
    {
      "source": "/bytteplan",
      "destination": "/bytteplan.html"
    },
    {
      "source": "/spilletid",
      "destination": "/spilletid.html"
    },
    {
      "source": "/kampdag",
      "destination": "/kampdag-seo.html"
    },
    {
      "source": "/treningsgrupper",
      "destination": "/treningsgrupper.html"
    },
    {
      "source": "/sesong",
      "destination": "/sesong.html"
    },
    {
      "source": "/stats/api/:match*",
      "destination": "https://api-gateway.umami.dev/api/:match*"
    },
    {
      "source": "/stats/:match*",
      "destination": "https://cloud.umami.is/:match*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
        { "key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()" },
        { "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" },
        { "key": "Content-Security-Policy", "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net https://cdnjs.cloudflare.com https://accounts.google.com https://cloud.umami.is https://js-de.sentry-cdn.com https://browser.sentry-cdn.com; style-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com https://fonts.googleapis.com; font-src 'self' https://cdnjs.cloudflare.com https://fonts.gstatic.com; img-src 'self' data: https:; connect-src 'self' https://*.supabase.co https://api.stripe.com https://accounts.google.com https://cdn.jsdelivr.net https://cloud.umami.is https://api-gateway.umami.dev https://*.ingest.de.sentry.io; frame-src https://js.stripe.com https://accounts.google.com; base-uri 'self'; form-action 'self' https://accounts.google.com" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [{ "key": "Cache-Control", "value": "no-store, max-age=0" }]
    },
    {
      "source": "/",
      "headers": [{ "key": "Cache-Control", "value": "no-store, max-age=0" }]
